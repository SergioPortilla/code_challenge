{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "types": ["astro/client"],
    "paths": {
      "@atom/*": ["src/components/atom/*"],
      "@molecule/*": ["src/components/molecule/*"],
      "@organism/*": ["src/components/organism/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
      "@core/*": ["src/core/*"]
    }
  }
}
